{
  "name": "fe-revend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run prettier && npm run eslint",
    "check": "npm run prettier:check && npm run eslint:check",
    "eslint": "eslint --ignore-path .gitignore --max-warnings=0 . './src/**/*.{js,jsx,ts,tsx,json}' --fix",
    "eslint:check": "eslint --ignore-path .gitignore --max-warnings=0 './src/**/*.{js,jsx,ts,tsx,json}' .",
    "prettier": "prettier --ignore-path .gitignore --write ./src",
    "prettier:check": "prettier --ignore-path .gitignore --check ./src",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.8.7",
    "@tanstack/react-query-devtools": "^5.8.7",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.293.0",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.48.2",
    "react-otp-input": "^3.1.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@next/eslint-plugin-next": "^14.0.3",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.8.4",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "postcss": "^8",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "npm run prettier",
      "npm run eslint",
      "git update-index --again"
    ],
    "./src/**/*.{json,css,scss,md}": [
      "npm run prettier",
      "git update-index --again"
    ]
  }
}
